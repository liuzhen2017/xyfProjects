package com.example.demo.ioc;

import org.springframework.stereotype.Component;

@Component
public class BeanA {

	
	BeanA(){
		System.out.println("=================================================");
	}
	
	private String name;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	private Integer id;
	
	
}
